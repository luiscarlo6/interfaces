<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luiscarlo.fotolingo" >

    <application
        android:allowBackup="true"
        android:theme="@style/AppTheme"
        android:debuggable="true">
        <activity
            android:name="com.example.luiscarlo.fotolingo.Inicio"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.luiscarlo.fotolingo.Principal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.luiscarlo.fotolingo.Principal" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.luiscarlo.fotolingo.Reto1"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.luiscarlo.fotolingo.Reto1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


    </application>

</manifest>
